IDI_OPENICON ICON "opendlg.ico"

STRINGTABLE
BEGIN
    IDS_OK,      "OK"
    IDS_CANCEL,  "Abbrechen"
    IDS_ERROR,   "Fehler"
    IDS_WARNING, "Warnung"
    IDS_HINT,    "Hinweis"
    IDS_YES,     "Ja"
    IDS_NO,      "Nein"
    IDS_ERRNR+0, "RC: %u\n beim Zugriff auf %s"
    IDS_ERRNR+1, "Ungültige Funktionsnummer"
    IDS_ERRNR+2, "%s\nnicht gefunden"
    IDS_ERRNR+3, "Verzeichnis nicht gefunden"
